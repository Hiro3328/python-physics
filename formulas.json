{
  "Física Básica": {
    "fma": {
      "names": ["a força", "a massa", "a aceleração"],
      "units": ["N", "Kg", "m/s²"],
      "formula": ["F = m*a", "m = F/a", "a = F/m"],
      "attributes": {"F": "", "m": "", "a": ""}
    },

    "sovt": {
      "names": ["a posição final", "a posição inicial", "a velocidade", "o tempo"],
      "units": ["m", "m", "m/s", "s"],
      "formula": ["S = S0 + V*t", "S0 = S - V*t", "V = (S - S0)/t", "t = (S - S0)/V"], 
      "attributes": {"S": "", "S0": "", "V": "", "t": ""}
    },

    "voat": {
      "names": ["a velocidade final", "a velocidade inicial", "a aceleração", "o tempo"],
      "units": ["m/s", "m/s", "m/s²", "s"],
      "formula": ["V = V0 + a*t", "V0 = V - a*t", "a = (V - V0)/t", "t = (V - V0)/a"], 
      "attributes": {"V": "", "V0": "", "a": "", "t": ""}
    }
  },

  "Astronomia":  { 
     "gGmr": {
       "names": ["a constante de gravitação universal", "a gravidade", "a massa" , "o raio"],
       "units": ["n*m²/kg²", "m/s²", "kg", "m"],
       "formula": ["g = G*m/r**2", "m = g*r**2/G", "r = (G*m/g)**0.5"],
       "attributes": {"G": "6.6743e-11", "g": "", "m": "", "r": ""}
    },
    "v2gmr": {
      "names": ["a velocidade", "a constante de gravitação universal", "a massa" , "o raio"],
      "units": ["m/s", "n*m²/kg²", "kg", "m"],
      "formula": ["v = (2*G*m/r)**0.5", "m = (v**2)*r/(2*G)", "r = (2*m*G)/v**2"],
      "attributes": {"v": "", "G": "6.6743e-11", "m": "", "r": ""}
    },
    "fgm": {
      "names": ["a força peso", "a gravidade", "a massa"],
      "units": ["N", "m/s²", "kg"],
      "formula": ["F = m*g", "g = F/m", "m = F/g"],
      "requires": {"g": ["Astronomia", "gGmr"]},
      "attributes": {"F": "", "g": "", "m": ""}
    }
  } 
}

